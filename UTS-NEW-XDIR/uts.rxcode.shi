#!/bin/bash

source uts.uts.shi
$(uts.return_if_read);

uts.val()
{
	echo $*;
}



rxcode.is()
{
	local val=$(uts.val $1);
	uts.isint $val && $((val >= 0)) && $((val < 256));
}

rxcode.assert()
{
	local val=$(val $1);
	uts.assert rxcode.is $val;
}

rxcode.init()
{
	local val=$(val $1);
	assert rxcode.is $val;
	uts.yield $val;
}

rxcode()
{
	echo $1;
}

